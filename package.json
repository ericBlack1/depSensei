{
  "name": "depsensei",
  "version": "0.1.0",
  "description": "A CLI tool to detect and resolve dependency issues across various programming environments",
  "main": "dist/index.js",
  "bin": {
    "depsensei": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:workflow": "ts-node test/workflow.test.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "dependencies",
    "dependency-management",
    "javascript",
    "npm",
    "yarn",
    "package-manager"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "npm-check-updates": "^16.14.17",
    "read-pkg": "^7.1.0",
    "semver": "^7.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/semver": "^7.5.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
